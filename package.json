{
  "name": "vue-doric-compiler",
  "version": "0.0.1",
  "description": "Compile vue template to doric",
  "main": "dist/index.js",
  "bin": {
    "vue2doric": "dist/index.js"
  },
  "scripts": {
    "start": "tsc && node dist/index.js",
    "clean": "rm -rf dist",
    "clean-win32": "del dist",
    "build": "tsc",
    "transform": "tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doric-pub/vue-doric-compiler.git"
  },
  "author": "pengfei.zhou",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/doric-pub/doric/issues"
  },
  "homepage": "https://doric.pub",
  "dependencies": {
    "@vue/component-compiler-utils": "^3.3.0",
    "@vue/consolidate": "^0.17.3",
    "commander": "^9.4.0",
    "estree-walker": "2.0.2",
    "magic-string": "^0.26.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "vue": "^2.7.7",
    "vue-template-compiler": "^2.7.7"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@babel/types": "^7.18.8",
    "@types/node": "^18.0.6",
    "tsc-alias": "^1.6.11",
    "typescript": "^4.7.4"
  }
}
